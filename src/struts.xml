<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<package name="ssh" extends="struts-default">
	<action name="helloworld" class="ssh.action.HelloWorldAction">
	    <result name="success">/WEB-INF/views/HelloWorld.jsp</result>
	</action>
	
	<action name="register" class="ssh.action.RegisterAction">
	    <result name="success">/WEB-INF/views/register.jsp</result>
	</action>
	
	<action name="student_*" class="ssh.action.StudentAction">
	    <result name="success">/WEB-INF/views/success.jsp</result>
	</action>
	
	
</package>
</struts> 